<div class="jumbotron">
      <div class="container">
        <h3>External Organizations</h3>
        <p></p>        
      </div>
</div>

<!--<div class="container">
	<%#= render 'form' %>
</div>-->

<div class="container">
	<table class="table table-hover">
		<thead>
			<tr>
				<th>Organization ID</th>
				<th>Organization Name</th>
				<!--<td>Sequence #</td>-->
				<th>Created On</th>
				<th>Updated On</th>
				<th>Actions</th>
			</tr>
		</thead>

		
		<tbody>
			<% @external_organizations.each do |f| %>
				<tr>
					<td><%= f.id %></td>
					<td><%= f.externalOrganizationName %></td>
					<!--<td><%= f.seqNo %></td>-->
					<td><%= f.created_at.strftime "%B %e, %Y" %></td>
					<td><%= f.updated_at.strftime "%B %e, %Y" %></td>
					<td class="actions">
						<%= link_to("Edit", "#", :class => 'action edit') %>
						<%= link_to("Delete", "#", :class => 'action delete') %>
					</td>
				</tr>
			<% end %>
		</tbody>
	</table>

	<hr>

      <footer>
        <p>&copy; 2016 Comcast</p>
      </footer>
    
</div>